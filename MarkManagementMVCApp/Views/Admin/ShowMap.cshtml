@model IEnumerable<MarkManagementMVCApp.Models.Mapping>

@{
    ViewBag.Title = "ShowMap";
    Layout = "~/Views/Admin/AdminMaster.cshtml";
}

@if (!Model.Any())

{
<h1 style="color:red;text-align:center">@ViewBag.info</h1>
}
else
{
<table class="table table-bordered table-hover"  style="width:60%;margin-left:270px;margin-top:100px">
    <tr>
        <th>
            CLASS ID
        </th>
        <th>
            FACULTY ID
        </th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClassName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FactName)
        </td>
       
    </tr>
}

</table>
}

